{
  "name": "front_screenshot",
  "version": "1.0.3",
  "description": "Mini library, with no dependencies, to output PNGs from maps and reports (or anything in the DOM)",
  "scripts": {
    "test": "grunt karma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuasoFoundries/front_screenshot.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "screenshot",
    "screenshooter",
    "browser screenshot",
    "html2canvas",
    "svg",
    "canvg"
  ],
  "author": "amenadiel@gmail.com",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/HuasoFoundries/front_screenshot/issues"
  },
  "homepage": "https://github.com/HuasoFoundries/front_screenshot#readme",
  "main": "dist/ig_screenshot.min.js",
  "module": "dist/ig_screenshot.es6.js",
  "jsnext:main": "dist/ig_screenshot.es6.js",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "canvg": "git+https://git@github.com/canvg/canvg.git",
    "eslint": "^3",
    "jspm": "0.17.0-beta.45",
    "karma": "^1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1",
    "karma-qunit": "^1",
    "lodash": "^4",
    "punycode": "^2.1.0",
    "qunitjs": "^1",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-ascii": "0.0.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "serve": "^6.3.1"
  },
  "jspm": {
    "registry": "jspm",
    "name": "front_screenshoot",
    "main": "ig_screenshot.min.js",
    "format": "amd",
    "directories": {
      "lib": "dist"
    },
    "dependencies": {},
    "devDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "html2canvas": "github:niklasvh/html2canvas@master",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "peerDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.25"
    },
    "overrides": {
      "github:niklasvh/html2canvas@master": {
        "registry": "npm"
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:pbkdf2@3.0.14": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.1": {
        "browser": "index.js"
      }
    }
  }
}
