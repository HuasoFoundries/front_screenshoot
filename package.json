{
  "name": "front_screenshot",
  "version": "0.9.3",
  "description": "Mini library, with no dependencies, to output PNGs from maps and reports (or anything in the DOM)",
  "scripts": {
    "start": "make run",
    "test": "karma start && MINIFIED=true karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuasoFoundries/front_screenshot.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "screenshot",
    "screenshooter",
    "browser screenshot",
    "html2canvas",
    "svg",
    "canvg"
  ],
  "author": "amenadiel@gmail.com",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/HuasoFoundries/front_screenshot/issues"
  },
  "homepage": "https://github.com/HuasoFoundries/front_screenshot#readme",
  "engines": {
    "node": "6.11.2"
  },
  "main": "dist/ig_screenshot.min.js",
  "module": "dist/ig_screenshot.es6.js",
  "jsnext:main": "dist/ig_screenshot.es6.js",
  "dependencies": {
    "jspm": "0.17.0-beta.45",
    "karma": "~1.7",
    "karma-phantomjs-launcher": "^1",
    "karma-qunit": "^1",
    "karma-mocha-reporter": "^2.2.5",
    "phantomjs-prebuilt": "^2",
    "qunitjs": "^1"
  },
  "devDependencies": {
    "babel-eslint": "^6",
    "eslint": "^3",
    "lodash": "^4"
  },
  "jspm": {
    "registry": "jspm",
    "name": "front_screenshot",
    "main": "ig_screenshot.min.js",
    "format": "amd",
    "directories": {
      "lib": "dist"
    },
    "dependencies": {},
    "devDependencies": {
      "canvg": "github:canvg/canvg@v1.4",
      "html2canvas": "github:niklasvh/html2canvas@^0.5.0-beta4",
      "punycode": "npm:jspm-nodelibs-punycode@^0.2.0"
    },
    "peerDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.25"
    },
    "overrides": {
      "github:canvg/canvg@v1.4": {
        "map": {
          "rgbcolor": "./rgbcolor.js",
          "stackblur": "./StackBlur.js"
        }
      },
      "github:niklasvh/html2canvas@0.5.0-beta4": {
        "main": "src/core.js",
        "format": "cjs"
      }
    }
  }
}
